{
  "QueueConfigurations": [
    {
      "Id": "IngestionQueue-Historical",
      "QueueArn": "arn:aws:sqs:us-east-1:611538926352:ventureos-ingestion-queue",
      "Events": ["s3:ObjectCreated:*"],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "prefix",
              "Value": "bronze/historical/"
            },
            {
              "Name": "suffix",
              "Value": ".csv"
            }
          ]
        }
      }
    },
    {
      "Id": "IngestionQueue-Daily",
      "QueueArn": "arn:aws:sqs:us-east-1:611538926352:ventureos-ingestion-queue",
      "Events": ["s3:ObjectCreated:*"],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "prefix",
              "Value": "bronze/daily/"
            },
            {
              "Name": "suffix",
              "Value": ".json"
            }
          ]
        }
      }
    }
  ],
  "LambdaFunctionConfigurations": [
    {
      "Id": "bedrock-batch-result-trigger",
      "LambdaFunctionArn": "arn:aws:lambda:us-east-1:611538926352:function:ventureos-bedrock-batch-result",
      "Events": [
        "s3:ObjectCreated:*"
      ],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "prefix",
              "Value": "bedrock-batches/output/"
            },
            {
              "Name": "suffix",
              "Value": ".jsonl"
            }
          ]
        }
      }
    }
  ]
}



