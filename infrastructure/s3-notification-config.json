{
  "LambdaFunctionConfigurations": [
    {
      "Id": "bedrock-batch-result-trigger",
      "LambdaFunctionArn": "arn:aws:lambda:us-east-1:611538926352:function:ventureos-bedrock-batch-result",
      "Events": [
        "s3:ObjectCreated:*"
      ],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "Prefix",
              "Value": "bedrock-batches/output/"
            },
            {
              "Name": "Suffix",
              "Value": ".jsonl"
            }
          ]
        }
      }
    },
    {
      "Id": "parquet-writer-trigger",
      "LambdaFunctionArn": "arn:aws:lambda:us-east-1:611538926352:function:ventureos-parquet-writer",
      "Events": [
        "s3:ObjectCreated:*"
      ],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "Prefix",
              "Value": "bronze/historical/"
            },
            {
              "Name": "Suffix",
              "Value": ".csv"
            }
          ]
        }
      }
    }
  ]
}



